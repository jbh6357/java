package Model;

public class BuildingDTO {
	
	private String owner;
    private String name;
    private int price;
    private int fee;
    private int t_price;

    public BuildingDTO(String name, int price, int fee, int t_price) {
        this.name = name;
        this.price = price;
        this.fee = fee;
        this.t_price = t_price;
    }

    public BuildingDTO() {}

    public String getName() {
        return name;
    }
    public int getPrice() {
        return price;
    }
    public int getFee() {
        return fee;
    }
    public int getT_price() {
        return t_price;
    }

    public void setName(String name) {
        this.name = name;
    }
    public void setPrice(int price) {
        this.price = price;
    }
    public void setFee(int fee) {
        this.fee = fee;
    }
    public void setT_price(int t_price) {
        this.t_price = t_price;
    }
}
