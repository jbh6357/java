package test;

public class ClearConsole {

    public static void main(String[] args) {
    	
    	
    	System.out.println("Hello World");
    	
    	for(int i=0; i<50; i++)
    	{
    		System.out.println();
    	}
    	
    	try {
			Thread.sleep(2000);
		} catch (InterruptedException e) {
			e.printStackTrace();
		}
    	
    	System.out.println("Hi");
    	
    	for(int i=0; i<20; i++)
    	{
    		System.out.println();
    	}
    	

    }

}
