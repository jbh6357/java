package test;

import java.io.IOException;

public class ClearConsole {

    public static void main(String[] args) throws IOException {
    	
    	
    	System.out.println("Hello World");
    	
    	try {
			Thread.sleep(2000);
		} catch (InterruptedException e) {
			e.printStackTrace();
		}
    	
    	System.out.println("Hi");

    }

}
